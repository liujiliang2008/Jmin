 </bean>

 <bean id="Job1">
  <class>org.jmin.job.xml.JobDefinition</class>
  <property name="name" value="Second Job" />
  <property name="className" value="org.jmin.test.job.TimeJob" />
  <property name="dateFormat" value="yyyy-MM-dd HH:mm:ss" />
  <property name="firstRunDateTime" value="2009-01-01 00:00:00" />
  <property name="interval" value="10" />
  <property name="intervalTimeUnit" value="Second" />
  <property name="supportMonthLastDay" value="true" />
 </bean>

